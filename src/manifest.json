{
    "name": "Interview Ready",
    "description": "This extension is designed to help you become interview ready!",
    "version": "0.16",
    "manifest_version": 3,
    "background": {
      "service_worker": "/service/background.js"
    },
    "content_scripts": [
      {
        "matches": ["https://leetcode.com/*"],
        "js": [
          "/shared/action-keys.js",
          "/shared/data-keys.js",
          "/shared/functions.js",
          "/onsite/content-script.js"
        ]
      }
    ],
    "permissions": [
        "storage", 
        "webRequest"
    ],
    "host_permissions": [
      "https://leetcode.com/*"
    ],
    "action": {
      "default_popup": "/ux/popup/home/home.html",
      "default_icon": {
        "16": "/ux/images/icon16.png",
        "32": "/ux/images/icon32.png",
        "48": "/ux/images/icon48.png",
        "128": "/ux/images/icon128.png"
      }
    },
    "icons": {
      "16": "/ux/images/icon16.png",
      "32": "/ux/images/icon32.png",
      "48": "/ux/images/icon48.png",
      "128": "/ux/images/icon128.png"
    },
    "options_page": "/ux/options/comingSoon.html"
}